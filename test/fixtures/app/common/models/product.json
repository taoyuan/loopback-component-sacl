{
	"name": "Product",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"id": {
			"type": "string",
			"id": true,
			"required": true
		},
		"name": {
			"type": "string",
			"required": true
		}
	},
	"validations": [],
	"relations": {
		"store": {
			"type": "belongsTo",
			"polymorphic": {
				"idType": "string"
			}
		}
	},
	"acls": [
		{
			"accessType": "*",
			"principalType": "ROLE",
			"principalId": "$everyone",
			"permission": "DENY"
		},
		{
			"accessType": "*",
			"principalType": "ROLE",
			"principalId": "$sacl",
			"permission": "ALLOW"
		}
	],
	"security": {

	},
	"methods": {}
}
